{
  "name": "passcheck",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.52",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.6.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react-scripts": "^5.0.1",
    "replace-in-file": "^7.1.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-rename && npm run copy-files-to-static && cd .. && python manage.py collectstatic",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-rename": "npm run build-rename-js && npm run build-rename-css && npm run build-fix-references && npm run build-rename-chunk-js",
    "build-rename-js": "renamer --find \"/main\\.[^\\.]+\\./i\" --replace \"passcheck.\" build\\static\\js\\*",
    "build-rename-css": "renamer --find \"/main\\.[^\\.]+\\./i\" --replace \"passcheck.\" build\\static\\css\\*",
    "build-rename-chunk-js": "renamer --find \"/787\\.[^\\.]+\\./i\" --replace \"passcheck.\" build\\static\\js\\*",
    "build-fix-references": "npm run build-fix-sourcemap && npm run build-fix-index && npm run build-fix-assetmanifest",
    "build-fix-sourcemap": "npm run build-fix-sourcemap-js && npm run build-fix-sourcemap-css && npm run build-fix-sourcemap-chunk-js",
    "build-fix-sourcemap-js": "replace-in-file \"/sourceMappingURL=main\\.[^\\.]+\\.js\\.map/i\" \"sourceMappingURL=passcheck.js.map\" build/static/js/passcheck.js --isRegex",
    "build-fix-sourcemap-css": "replace-in-file \"/sourceMappingURL=main\\.[^\\.]+\\.css\\.map/i\" \"sourceMappingURL=passcheck.css.map\" build/static/css/passcheck.css --isRegex",
    "build-fix-sourcemap-chunk-js": "replace-in-file \"/sourceMappingURL=787\\.[^\\.]+\\.chunk\\.js\\.map/i\" \"sourceMappingURL=passcheck.chunk.js.map\" build/static/js/passcheck.chunk.js --isRegex",
    "build-fix-index": "npm run build-fix-index-js && npm run build-fix-index-css && npm run build-fix-index-css-chunk",
    "build-fix-index-js": "replace-in-file \"/main\\.[^\\.]+\\.js/i\" \"passcheck.js?v=%npm_package_version%\" build/index.html --isRegex",
    "build-fix-index-css": "replace-in-file \"/main\\.[^\\.]+\\.css/i\" \"passcheck.css?v=%npm_package_version%\" build/index.html --isRegex",
    "build-fix-index-css-chunk": "replace-in-file \"/787\\.[^\\.]+\\.chunk\\js/i\" \"passcheck.chunk.js?v=%npm_package_version%\" build/index.html --isRegex",
    "build-fix-assetmanifest": "npm run build-fix-assetmanifest-js && npm run build-fix-assetmanifest-css && npm run build-fix-assetmanifest-js-map && npm run build-fix-assetmanifest-css-map && npm run build-fix-assetmanifest-chunk-css && npm run build-fix-assetmanifest-chunk-css-map",
    "build-fix-assetmanifest-js": "replace-in-file \"/main\\.[^\\.]+\\.js/i\" \"passcheck.js\" build/asset-manifest.json --isRegex",
    "build-fix-assetmanifest-css": "replace-in-file \"/main\\.[^\\.]+\\.css/i\" \"passcheck.css\" build/asset-manifest.json --isRegex",
    "build-fix-assetmanifest-js-map": "replace-in-file \"/main\\.[^\\.]+\\.js\\.map/i\" \"passcheck.js.map\" build/asset-manifest.json --isRegex",
    "build-fix-assetmanifest-css-map": "replace-in-file \"/main\\.[^\\.]+\\.css\\.map/i\" \"passcheck.css.map\" build/asset-manifest.json --isRegex",
    "build-fix-assetmanifest-chunk-css": "replace-in-file \"/787\\.[^\\.]+\\.chunk\\.js/i\" \"passcheck.chunk.js\" build/asset-manifest.json --isRegex",
    "build-fix-assetmanifest-chunk-css-map": "replace-in-file \"/787\\.[^\\.]+\\.chunk\\.js\\.map/i\" \"passcheck.chunk.js.map\" build/asset-manifest.json --isRegex",
    "copy-files-to-static": "xcopy /Y /S /EXCLUDE:exclude-files.txt build\\static\\* static\\passcheck"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.22.11",
    "copy-files": "^0.1.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "renamer": "^4.0.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
